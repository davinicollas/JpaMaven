package Application;

import javax.persistence.EntityManager;
import javax.persistence.EntityManagerFactory;
import javax.persistence.Persistence;

import Dominio.Pessoa;

public class Programa {

	public static void main(String[] args) {

		Pessoa p1 = new Pessoa(1, "carlos da silva", "calos@hotmail.com");
		Pessoa p2 = new Pessoa(2, "Joaquim torres", "joaquim@hotmail.com");
		Pessoa p3 = new Pessoa(3, "tevez", "tevez@hotmail.com");
		
		EntityManagerFactory enf = Persistence.createEntityManagerFactory("exemplo-jpa");
		EntityManager em = enf.createEntityManager();
		System.out.println(p1);
		System.out.println(p2);
		System.out.println(p3);


	}

}
